<Window x:Class="Prb.ArraysRandomGetallen.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prb.ArraysRandomGetallen.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="btnVersion1Add" Content="Versie 1 optellen" HorizontalAlignment="Left" Height="35" Margin="24,10,0,0" VerticalAlignment="Top" Width="153" Click="btnVersion1Add_Click" />
        <Button x:Name="btnVersion2Add" Content="Versie 2 optellen" HorizontalAlignment="Left" Height="35" Margin="305,10,0,0" VerticalAlignment="Top" Width="154" Click="btnVersion2Add_Click"/>
        <ListBox x:Name="lstDisplay" HorizontalAlignment="Left" Height="318" Margin="24,96,0,0" VerticalAlignment="Top" Width="435"/>
        <Button x:Name="btnVersion1Multiply" Content="Versie 1 vermenigvuldigen" HorizontalAlignment="Left" Height="35" Margin="24,50,0,0" VerticalAlignment="Top" Width="153" Click="btnVersion1Multiply_Click" />
        <Button x:Name="btnVersion2Multiply" Content="Versie 2 vermenigvuldigen" HorizontalAlignment="Left" Height="35" Margin="305,50,0,0" VerticalAlignment="Top" Width="154" Click="btnVersion2Multiply_Click"/>

    </Grid>
</Window>
